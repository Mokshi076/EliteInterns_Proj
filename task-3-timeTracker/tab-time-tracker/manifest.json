{
  "manifest_version": 3,
  "name": "Simple Tab Time Tracker",
  "version": "1.0",
  "description": "Tracks time spent on each website",
  "permissions": ["tabs", "storage","alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
